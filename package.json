{
  "name": "moentix",
  "version": "1.0.0",
  "description": "Gestor de gastos personal minimalista",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend": "cd backend && npx ts-node src/index.ts",
    "backend:dev": "cd backend && npx nodemon --exec ts-node src/index.ts",
    "dev": "concurrently \"npm run backend:dev\" \"npm start\"",
    "install:all": "npm install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "axios": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "expo": "~53.0.0",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "express": "^4.19.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.0",
    "nativewind": "^4.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.0",
    "@types/react": "~19.0.10",
    "@types/react-dom": "^19.1.5",
    "@types/react-native": "~0.73.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "react-native-web": {
      "react": "19.0.0",
      "react-dom": "19.0.0"
    },
    "react-dom": "19.0.0",
    "@types/react": "~19.0.10"
  },
  "resolutions": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "@types/react": "~19.0.10"
  },
  "private": true
}
