{
  "name": "moentix",
  "version": "1.0.0",
  "description": "Gestor de gastos personal minimalista",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend": "cd backend && npx ts-node src/index.ts",
    "backend:dev": "cd backend && npx nodemon --exec ts-node src/index.ts",
    "dev": "concurrently \"npm run backend:dev\" \"npm start\"",
    "install:all": "npm install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "axios": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "expo": "~51.0.0",
    "expo-constants": "~16.0.0",
    "expo-font": "~12.0.0",
    "expo-linking": "~6.3.0",
    "expo-router": "~3.5.0",
    "expo-splash-screen": "~0.27.0",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.0",
    "express": "^4.19.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.0",
    "nativewind": "^4.1.23",
    "react": "18.2.0",
    "react-native": "^0.74.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "tslib": "^2.6.2"
  },
