{
  "expo": {
    "name": "monetixx",
    "slug": "monetixx",
    "version": "1.0.0",
    "scheme": "monetixx",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ralento.mvk.monetixx"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "73b19cf2-9d12-45b1-9b3f-e38bbf5bd9f9"
      }
    }
  }
}
