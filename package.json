    "@babel/core": "^7.20.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.0",
    "@types/react": "~18.2.79",
    "@types/react-native": "~0.73.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
